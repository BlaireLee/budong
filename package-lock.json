{
  "name": "budong-academy",
  "version": "0.1.0",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "name": "budong-academy",
      "version": "0.1.0",
      "workspaces": [
        "apps/*",
        "packages/*"
      ],
      "engines": {
        "node": ">=18.8.0"
      }
    },
    "apps/api": {
      "name": "@budong/api",
      "version": "0.1.0"
    },
    "apps/web": {
      "name": "@budong/web",
      "version": "0.1.0"
    },
    "node_modules/@budong/api": {
      "resolved": "apps/api",
      "link": true
    },
    "node_modules/@budong/domain": {
      "resolved": "packages/domain",
      "link": true
    },
    "node_modules/@budong/shared-types": {
      "resolved": "packages/shared-types",
      "link": true
    },
    "node_modules/@budong/ui": {
      "resolved": "packages/ui",
      "link": true
    },
    "node_modules/@budong/web": {
      "resolved": "apps/web",
      "link": true
    },
    "packages/domain": {
      "name": "@budong/domain",
      "version": "0.1.0"
    },
    "packages/shared-types": {
      "name": "@budong/shared-types",
      "version": "0.1.0"
    },
    "packages/ui": {
      "name": "@budong/ui",
      "version": "0.1.0"
    }
  },
  "dependencies": {
    "@budong/api": {
      "version": "file:apps/api"
    },
    "@budong/domain": {
      "version": "file:packages/domain"
    },
    "@budong/shared-types": {
      "version": "file:packages/shared-types"
    },
    "@budong/ui": {
      "version": "file:packages/ui"
    },
    "@budong/web": {
      "version": "file:apps/web"
    }
  }
}
