{
  "version": 2,
  "installCommand": "npm ci",
  "buildCommand": "npm run build:web",
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/v1/:path*",
      "destination": "/api"
    },
    {
      "source": "/healthz",
      "destination": "/api"
    },
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
